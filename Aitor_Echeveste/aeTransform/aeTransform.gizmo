  name Crop3
  xpos 309
  ypos 747
 }
 Reformat {
  name Reformat2
  xpos 309
  ypos 771
  disable {{!cropformat}}
 }
 BlackOutside {
  name BlackOutsideAfter
  xpos 309
  ypos 809
  disable {{!blackoutsideafter}}
 }
 Output {
